# ==========================
# Frontend Settings
# Max upload size in MB for frontend (keep in sync with Nginx settings)
VITE_MAX_UPLOAD_MB=100

# ==========================
# Database (MySQL)
DB_HOST=

# MySQL port (default 3306)
DB_PORT=3306

# Database username with required privileges
DB_USER=

# Database user's password
DB_PASSWORD=

# Database name (will be auto-created on first run if missing)
DB_NAME=dashboard

# ==========================
# Power BI Service Principal (Backend)

# Azure AD tenant ID 
PBI_TENANT_ID=

# Azure AD application (client) ID
PBI_CLIENT_ID=

# Azure AD application client secret
PBI_CLIENT_SECRET=

# Power BI workspace ID 
PBI_WORKSPACE_ID=

# Power BI dataset ID
PBI_DATASET_ID=

# Server-side embed URL used by backend
PBI_EMBED_URL=

